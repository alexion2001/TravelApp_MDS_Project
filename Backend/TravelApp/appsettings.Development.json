{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JWTToken": {
    "RefreshTokenConfigs": {
      "RefreshToken": {
        "NumberOfRefreshes": "3",
        "TimeLeftUntilRefreshTokenExpiresAfterTokenAlreadyExpired": "5",
        "Issuer": "https://localhost:5001/",
        "Audience": "https://localhost:5001/"
      }
    },
    "LoginTokenConfigs": {
      "LoginToken": {
        "Minutes": "30",
        "Hours": "12",
        "Days": "1"
      }
    }
  }

}